{
  "name": "pulsar-lib",
  "version": "1.0.0",
  "description": "Configuration values and functionality shared by pulsar-api and pulsar-ui",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "node ./src/app.js",
    "build": "npm run srcCompile",
    "srcCompile": "tsc -p .",
    "pretest": "tslint \"src/**/*.ts\" \"test/**/*.ts\" && tsc -p ./",
    "test": "echo \"Warning: no Tests\"",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "watch": "grunt watch",
    "buildRun": "tsc -p ./ && node ./src/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://thunder033@github.com/thunder033/pulsar-lib.git"
  },
  "author": "Greg Rozmarynowycz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thunder033/pulsar-lib/issues"
  },
  "homepage": "https://pulsar-api.herokuapp.com/",
  "dependencies": {
    "@types/express": "^4.0.35",
    "@types/node": "^7.0.5",
    "@types/q": "0.0.32",
    "@types/socket.io": "^1.4.28",
    "@types/uuid": "^2.0.29",
    "express": "^4.14.1",
    "q": "^1.4.1",
    "socket.io": "^1.7.2",
    "typescript": "2.1.6",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.39",
    "chai": "^3.5.0",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-compress": "^1.4.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "heroku-deploy-tarball": "^1.0.0",
    "mocha": "^3.2.0",
    "mocha-typescript": "^1.0.22",
    "tslint": "^4.5.1"
  }
}
